{
  "name": "@loom/shuttle",
  "version": "0.1.0",
  "description": "Shuttle - CLI tool for Loom multi-agent infrastructure",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "shuttle": "./dist/index.js"
  },
  "scripts": {
